   <xs:complexType name="borderFunctionLocationBorderType">
      <xs:complexContent>
         <xs:extension base="extensibleElementType">
            <xs:sequence>
               <xs:element name="layers" type="borderFunctionLocationBorderLayersType"/>
            </xs:sequence>
            <xs:attribute name="lineId" type="xs:string" use="optional"/>
            <xs:attribute name="trackId" type="xs:string" use="optional"/>
            <xs:attribute name="lineSectionId" type="xs:string" use="optional"/>
            <xs:attribute name="normalDrivingDirection" use="optional">
               <xs:annotation>
                  <xs:documentation>What is the normal driving direction, expressed with respect to this node's
                     ports? See http://wiki-artemis/index.php/NormalDrivingDirection for details.
                  </xs:documentation>
               </xs:annotation>
               <xs:simpleType>
                  <xs:restriction base="xs:string">
                     <xs:enumeration value="up">
                        <xs:annotation>
                           <xs:documentation>From port 1 to port 2.</xs:documentation>
                        </xs:annotation>
                     </xs:enumeration>
                     <xs:enumeration value="down">
                        <xs:annotation>
                           <xs:documentation>From port 2 to port 1.</xs:documentation>
                        </xs:annotation>
                     </xs:enumeration>
                     <xs:enumeration value="up+down">
                        <xs:annotation>
                           <xs:documentation>Both directions are equally preferred.</xs:documentation>
                        </xs:annotation>
                     </xs:enumeration>
                  </xs:restriction>
               </xs:simpleType>
            </xs:attribute>
         </xs:extension>
      </xs:complexContent>
